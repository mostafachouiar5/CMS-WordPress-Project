{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpengine-smart-search/dropdown-facet",
  "version": "0.1.0",
  "title": "Dropdown Facet",
  "category": "wpengine-smart-search",
  "icon": "dropdown",
  "description": "WP Engine Smart Search block for categories dropdown",
  "example": {},
  "supports": {
    "html": false,
    "interactivity": true,
    "align": [
      "wide",
      "full"
    ],
    "anchor": true,
    "className": true
  },
  "attributes": {
    "hasLabel": {
      "type": "boolean",
      "default": true
    },
    "label": {
      "type": "string",
      "default": "Dropdown Label"
    },
    "placeholder": {
      "type": "string",
      "default": "Dropdown..."
    },
    "formId": {
      "type": "string",
      "default": ""
    }
  },
  "keywords": [
    "wpengine",
    "WP Engine",
    "smart search",
    "smartsearch",
    "Smarm Search",
    "dropdown",
    "categories"
  ],
  "textdomain": "wpengine-smart-search",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewStyle": "file:./style-index.css",
  "viewScriptModule": "file:./view.js",
  "render": "file:./render.php"
}